<h3> Problem Report Listing </h3>

<table>
    <tr> 
        <th> Id </th>
        <th> Reporter Name </th>
        <th> Venue </th>
        <th> PC Number </th>
        <th> Critical? </th>
        <th> Blocked? (AS8 Only) </th>
        <th> Fixed? </th>
        <th> Fixable? </th>
        <th> Description </th>
        <th> Remarks </th>
        <th> Last Updated By </th>
        <th> Last Updated At </th>
    <tr>
    
    <% @problemReports.each |pb| do %>
        <tr>
            <th> <% pb.id %> </th>
            <th> Reporter Name </th>
            <th> Venue </th>
            <th> PC Number </th>
            <th> Critical? </th>
            <th> Blocked? (AS8 Only) </th>
            <th> Fixed? </th>
            <th> Fixable? </th>
            <th> Description </th>
            <th> Remarks </th>
            <th> Last Updated By </th>
            <th> Last Updated At </th>
        </tr>
    <% end %>
</table>